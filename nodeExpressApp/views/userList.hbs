<h1>{{title}}</h1>
<table class="table">
    <tr>
        <td>Name</td>
        <td>Go to profile</td>
    </tr>
    {{#each allUsers}}
    <tr>
        <td>{{this.firstName}} {{this.lastName}}</td>
        <td>
            <a href="/users/profile/{{this.id}}">Go to profile</a>
        </td>
    </tr>
    {{/each}}
</table>
